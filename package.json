    "files": [
        "build",
        "src/css/litegraph.css",
        "src/litegraph.d.ts",
        "src/index.d.ts"
    ],
    "bugs": {
        "url": "https://github.com/daniel-lewis-ab/litegraph.js/issues"
    },
    "homepage": "https://github.com/daniel-lewis-ab/litegraph.js#readme",
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@stylistic/eslint-plugin": "^5.8.0",
        "eslint": "^10.0.0",
        "jest-cli": "^30.2.0",
        "jsdoc": "^4.0.5",
        "nodemon": "^3.1.11",
        "vite": "^7.3.1"
    },
    "keywords": [
